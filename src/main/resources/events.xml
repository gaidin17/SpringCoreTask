<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
 		http://www.springframework.org/schema/beans
 		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c">

    <bean id="eventDaoImpl" class="DAO.Impl.EventDaoImpl">
        <constructor-arg>
            <list>
                <ref bean="event1" />
                <ref bean="user2" />
                <ref bean="user3" />
                <ref bean="user4" />
            </list>
        </constructor-arg>
    </bean>

    <bean id="event1" class="Domain.Event">
        <constructor-arg type="java.lang.String" value="beerFest"/>
        <constructor-arg type="java.lang.String" value="2016-10-05"/>
        <constructor-arg type="java.lang.String" value="21:30"/>
        <constructor-arg type="java.lang.String" value="1200.00" />
        <constructor-arg type="java.lang.String" value="1200.00" />
        <constructor-arg type="java.lang.String" value="1200.00" />
    </bean>


    <bean id="ticket" class="DAO.Impl.EventDaoImpl">
        <constructor-arg>
            <list>
                <ref bean="event1" />
                <ref bean="user2" />
                <ref bean="user3" />
                <ref bean="user4" />
            </list>
        </constructor-arg>
    </bean>


</beans>
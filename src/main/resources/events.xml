<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
 		http://www.springframework.org/schema/beans
 		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c">

    <bean id="event1" class="domain.Event">
        <constructor-arg type="java.lang.String" value="beerFest"/>
        <constructor-arg type="java.lang.String" value="2016-10-05"/>
        <constructor-arg type="java.lang.String" value="21:30"/>
        <constructor-arg type="java.lang.Double" value="1200.00"/>
        <constructor-arg value="HIGHT"/>
        <constructor-arg ref="auditorium1"/>
    </bean>

    <bean id="event2" class="domain.Event">
        <constructor-arg type="java.lang.String" value="rockEvening"/>
        <constructor-arg type="java.lang.String" value="2016-08-10"/>
        <constructor-arg type="java.lang.String" value="22:00"/>
        <constructor-arg type="java.lang.Double" value="1000.00"/>
        <constructor-arg value="LOW"/>
        <constructor-arg ref="auditorium2"/>
    </bean>

    <bean id="event3" class="domain.Event">
        <constructor-arg type="java.lang.String" value="cinemaNight"/>
        <constructor-arg type="java.lang.String" value="2016-09-01"/>
        <constructor-arg type="java.lang.String" value="23:30"/>
        <constructor-arg type="java.lang.Double" value="800.00"/>
        <constructor-arg value="MID"/>
        <constructor-arg ref="auditorium3"/>
    </bean>

    <bean id="event4" class="domain.Event">
        <constructor-arg type="java.lang.String" value="newYear"/>
        <constructor-arg type="java.lang.String" value="2016-12-31"/>
        <constructor-arg type="java.lang.String" value="22:30"/>
        <constructor-arg type="java.lang.Double" value="5000.00"/>
        <constructor-arg value="HIGHT"/>
        <constructor-arg ref="auditorium4"/>
    </bean>
</beans>